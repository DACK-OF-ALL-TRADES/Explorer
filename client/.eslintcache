\n                style={{ marginTop: \"1rem\" }}\r\n                color=\"red\"\r\n                onClick={() => handleDeleteCity()}\r\n              >\r\n                Remove\r\n              </Button>\r\n            </div>\r\n            <div className=\"city-result-title\">\r\n              <h1>My Favorites</h1>\r\n            </div>\r\n            <div className=\"search-city-display\">\r\n              <Card.Group centered>\r\n                {favCities.map((cityInfo) => {\r\n                  if (cityInfo.image === \"\") {\r\n                    return (\r\n                      <Card\r\n                        image={\r\n                          \"https://cdn.pixabay.com/photo/2015/03/26/09/48/buildings-690364__340.jpg\"\r\n                        }\r\n                        header={cityInfo.city}\r\n                        meta={cityInfo.country}\r\n                        className=\"city-card\"\r\n                        key={cityInfo.city}\r\n                        href={\"/search/\" + cityInfo.id}\r\n                        color=\"red\"\r\n                      />\r\n                    );\r\n                  } else {\r\n                    return (\r\n                      <Card\r\n                        image={cityInfo.image}\r\n                        header={cityInfo.city}\r\n                        meta={cityInfo.country}\r\n                        className=\"city-card\"\r\n                        key={cityInfo.city}\r\n                        href={\"/search/\" + cityInfo.id}\r\n                        color=\"red\"\r\n                      />\r\n                    );\r\n                  }\r\n                })}\r\n              </Card.Group>\r\n            </div>\r\n          </div>\r\n        )}\r\n      </div>\r\n      <Footer />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default MyFavorites;\r\n","C:\\Users\\kemal\\Desktop\\code\\_repositories\\explorer\\client\\src\\pages\\Continents\\index.js",[],"C:\\Users\\kemal\\Desktop\\code